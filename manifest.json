{
  "manifest_version": 3,
  "name": "Friendly Extension SUAP UEPB",
  "version": "1.0",
  "description": "Uma extensão amigável para o SUAP UEPB",
  "permissions": [
    "activeTab",
    "storage"
  ],

  "content_scripts": [
    {
      "matches": ["https://suap.uepb.edu.br/*"],
      "js": ["content.js"]
    }
  ]
}
